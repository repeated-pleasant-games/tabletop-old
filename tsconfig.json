{
    "compilerOptions": {
        "outDir": "./build/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "es6",
        "target": "es6",
        "jsx": "react",
        "esModuleInterop": true,
        "moduleResolution": "node",
        "baseUrl": "./",
        "paths": {
          "@/*": [ "src/*" ]
        }
    },
    "exclude": [ "node_modules", "dist" ]
}